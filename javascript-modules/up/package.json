{
  "name": "@bookshop/up",
  "packageManager": "pnpm@10.11.0",
  "version": "3.18.0-alpha.1",
  "description": "Bookshop up allows you to upgrade Bookshop versions and file syntax automatically",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "test": "nyc ava -v"
  },
  "bin": {
    "bookshop-up": "main.js"
  },
  "files": [
    "**/*.js",
    "!**/*.test.js",
    "!.test/**/*"
  ],
  "author": "@bglw",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "ava": "^6.4.1",
    "nyc": "^17.1.0"
  },
  "dependencies": {
    "@bookshop/helpers": "workspace:*",
    "@ltd/j-toml": "^1.30.0",
    "chalk": "^5.0.1",
    "commander": "^8.1.0",
    "fast-glob": "^3.2.7",
    "inquirer": "^8.2.1",
    "normalize-path": "^3.0.0",
    "pluralize": "^8.0.0",
    "yaml": "^1.10.2"
  },
  "engines": {
    "node": ">=14.16"
  }
}
