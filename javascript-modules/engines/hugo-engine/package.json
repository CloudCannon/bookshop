{
  "name": "@bookshop/hugo-engine",
  "packageManager": "pnpm@10.11.0",
  "version": "3.18.0-alpha.1",
  "description": "Bookshop frontend Hugo renderer",
  "type": "module",
  "sideEffects": false,
  "main": "main.js",
  "exports": {
    ".": "./main.js",
    "./build": "./build.js"
  },
  "files": [
    "*.js",
    "lib/*.js",
    "!**/*.test.js",
    "!**/*__test__.js",
    "full-hugo-renderer/hugo_renderer.wasm.gz",
    "full-hugo-renderer/wasm_exec.js",
    "bookshop-hugo-templates/**/*"
  ],
  "scripts": {
    "test": "node prep__test__.js && nyc ava -v"
  },
  "author": "@bglw",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "ava": "^6.4.1",
    "nyc": "^17.1.0"
  },
  "dependencies": {
    "@bookshop/helpers": "workspace:*",
    "esbuild": "^0.19.3",
    "fflate": "^0.7.3",
    "liquidjs": "10.17.0"
  },
  "engines": {
    "node": ">=14.16"
  }
}
